{
    "$schema": "https://turbo.build/schema.json",
    "concurrency": "50%",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**"
            ]
        },
        "test": {
            "dependsOn": [
                "build"
            ]
        }
    }
}